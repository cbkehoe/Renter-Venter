<table id="list">
  
    <h1>Available Apartments</h1>
    <thead>
        <tr>
          <th>Apartment Name</th>
          
          <th>Address</th>
          <th>Vacany</th>
          <th></th>
        </tr>
        <tbody>
          <tr>
        <% rents.forEach(function(r) { %>
        <td><%=r.apartment %></td>
        <td><%= r.address %></td>
        <td><%= r.vacant ? 'Yes' : 'No' %></td>
        <td><a href='/rents/<%=r._id %>'>Vents</td>
        </tr>
      <% }); %>

      


    </tbody>
  </table>